<%= render 'pages/home' if !user_signed_in? %>
<div id="pins" class="transitions-enabled">
	<% @pins.each do |pin| %>
		<div class="box panel panel-default">
	    	
	    	<%= link_to image_tag(pin.image.url), pin,  class: "index-image" %>
	    	
	    	<div class="panel-body">
	    	<strong><%= pin.description %></strong>
	    	<p style="font-size: 9px;"><%= pin.user.email if pin.user %></p>
	    	
	    	</div>
	    </div>
	<% end %>
</div>
<%= will_paginate @posts, class: "pagination-sm" , renderer: BootstrapPagination::Rails  %>
